{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "tags": [
        "-lintignore"
    ],
    "ignore": [
        "patches/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/.nuxt/**",
        "**/lib/**",
        "**/build/**"
    ],
    "ignoreDependencies": ["eslint", "@eslint/js", "@eslint/eslintrc", "globals"],
    "paths": {
        "@/*": ["packages/client/src/*", "packages/server/src/*"],
        "@interface/*": ["packages/interface/src/*"],
        "@static/*": ["packages/common/static/*"],
        "@data/*": ["packages/common/data/*"],
        "@common/*": ["packages/common/src/*"],
        "@model/*": ["packages/model/data/*"],
        "@search/*": ["packages/search/src/*"],
        "@search-data/*": ["packages/search/data/*"],
        "@server/*": ["packages/server/src/*"]
    },
    "workspaces": {
        ".": {
            "entry": [
                "packages/server/src/app.ts",
                "packages/client/src/App.vue",
                "packages/codegen/scripts/gen-model.ts",
                "packages/*/src/index.ts"
            ],
            "project": [
                "packages/*/src/**/*.{ts,js,vue}",
                "packages/*/scripts/**/*.{ts,js}",
                "!packages/*/src/**/*.test.{ts,js}",
                "!packages/*/src/**/*.spec.{ts,js}",
                "!packages/*/lib/**",
                "!packages/*/dist/**"
            ]
        },
        "packages/server": {
            "entry": [
                "src/app.ts"
            ],
            "project": [
                "src/**/*.ts",
                "!src/**/*.test.ts",
                "!src/**/*.spec.ts"
            ],
            "ignoreDependencies": [
                "@types/*"
            ]
        },
        "packages/client": {
            "entry": [
                "quasar.config.js",
                "src/App.vue",
                "src/boot/**/*.{ts,js}",
                "src/router/**/*.{ts,js}",
                "src/layouts/**/*.vue",
                "src/pages/**/*.vue"
            ],
            "project": [
                "src/**/*.{ts,js,vue}",
                "quasar.config.js",
                "!src/**/*.test.{ts,js}",
                "!src/**/*.spec.{ts,js}"
            ],
            "ignoreDependencies": [
                "@types/*",
                "@quasar/extras"
            ]
        },
        "packages/common": {
            "entry": [
                "src/index.ts"
            ],
            "project": [
                "src/**/*.ts"
            ]
        },
        "packages/interface": {
            "entry": [
                "src/index.ts"
            ],
            "project": [
                "src/**/*.ts"
            ]
        },
        "packages/model": {
            "entry": [
                "src/index.ts"
            ],
            "project": [
                "src/**/*.ts"
            ]
        },
        "packages/search": {
            "entry": [
                "src/index.ts"
            ],
            "project": [
                "src/**/*.ts"
            ]
        },
        "packages/renderer": {
            "entry": [
                "src/index.ts"
            ],
            "project": [
                "src/**/*.ts"
            ]
        },
        "packages/codegen": {
            "entry": [
                "scripts/gen-model.ts"
            ],
            "project": [
                "scripts/**/*.ts",
                "src/**/*.ts"
            ]
        },
        "packages/log-parser": {
            "entry": [
                "lib/index.js"
            ],
            "project": [
                "lib/**/*.js",
                "src/**/*.ts"
            ]
        }
    }
}
