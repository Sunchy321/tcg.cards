{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "noImplicitAny": true,
        "outDir": "./dist",
        "sourceMap": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "lib": ["ES2020"],
        "baseUrl": "./src",
        "paths": {
            "*": ["@types/*"],
            "@/*": ["./*"],
            "@static": ["./static"],
            "@data/*": ["../data/*"],
            "@interface/*": ["../node_modules/card-interface/*"]
        }
    },
    "include": ["src/*"]
}
