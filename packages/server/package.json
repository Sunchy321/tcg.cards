{
    "name": "card-server",
    "version": "1.0.2",
    "description": "Card Server",
    "scripts": {
        "lint": "eslint ./src ./data --ext js,ts --cache --fix",
        "dev": "bun --watch --inspect ./src/app.ts",
        "build": "bun build ./src/app.ts --target=bun --outdir=./dist",
        "gen-openapi": "node -r ts-node/register -r tsconfig-paths/register scripts/gen-doc.ts",
        "gen-auth": "bunx @better-auth/cli generate --config src/auth/index.ts --output src/auth/schema.ts -y",
        "gen-model": "tsx scripts/gen-model.ts && bunx eslint src --fix",
        "gen-router": "tsc --emitDeclarationOnly --outDir decls && cp ./decls/src/router.d.ts ../client/src/router.d.ts"
    },
    "homepage": "https://github.com/Sunchy321/tcg.cards#readme",
    "license": "MIT",
    "author": "Sunchy321 Sunchy321 <sunchy321@qq.com>",
    "main": "src/app.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sunchy321/tcg.cards.git"
    },
    "bugs": {
        "url": "https://github.com/Sunchy321/tcg.cards/issues"
    },
    "type": "module",
    "devDependencies": {
        "@better-auth/cli": "^1.3.4",
        "@eslint/eslintrc": "^3.3.0",
        "@eslint/js": "^9.22.0",
        "@types/ahocorasick": "^1.0.0",
        "@types/bun": "^1.3.8",
        "@types/cheerio": "^1.0.0",
        "@types/deep-diff": "^1.0.0",
        "@types/diff": "^4.0.2",
        "@types/js-yaml": "^4.0.9",
        "@types/lodash": "^4.17.16",
        "@types/node": "^14.14.14",
        "@types/pg": "^8.15.4",
        "@types/ws": "^7.4.0",
        "drizzle-kit": "^0.31.4",
        "eslint": "^9.22.0",
        "globals": "^16.0.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^3.9.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
    },
    "dependencies": {
        "@hono/bun-compress": "^0.1.0",
        "@hono/prometheus": "^1.0.2",
        "@hono/zod-validator": "^0.7.2",
        "@orpc/json-schema": "^1.8.1",
        "@orpc/openapi": "^1.8.1",
        "@orpc/server": "^1.8.1",
        "@orpc/zod": "^1.8.1",
        "@tcgdex/sdk": "^2.7.1",
        "ahocorasick": "^1.0.2",
        "ai": "^6.0.31",
        "axios": "^1.8.4",
        "axios-retry": "^4.0.0",
        "better-auth": "^1.3.4",
        "card-common": "workspace:*",
        "card-model": "workspace:*",
        "card-search": "workspace:*",
        "cheerio": "^1.0.0-rc.3",
        "diff": "^5.0.0",
        "dotenv": "^17.2.1",
        "drizzle-orm": "^0.44.2",
        "hono": "^4.8.10",
        "hono-openapi": "^0.4.8",
        "hs-log-parser": "^0.0.2",
        "js-yaml": "^4.1.0",
        "jsondiffpatch": "^0.7.3",
        "lodash": "^4.17.20",
        "logform": "^2.3.0",
        "nanoid": "^5.1.6",
