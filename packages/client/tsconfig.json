{
    "extends": "./.quasar/tsconfig.json",
    "compilerOptions": {
        "target": "ES2019",
        "baseUrl": ".",
        "jsx": "preserve",
        "types": ["vite/client", "@intlify/unplugin-vue-i18n/messages"],
        "paths": {
            "src/*": ["src/*"],
            "app/*": ["*"],
            "components/*": ["src/components/*"],
            "layouts/*": ["src/layouts/*"],
            "pages/*": ["src/pages/*"],
            "assets/*": ["src/assets/*"],
            "boot/*": ["src/boot/*"],
            "setup/*": ["src/setup/*"],
            "store/*": ["src/stores/*"],
            "interface/*": ["node_modules/card-interface/src/*"],
            "@interface/*": ["node_modules/card-interface/src/*"],
            "static/*": ["node_modules/card-common/static/*"],
            "data/*": ["node_modules/card-common/data/*"],
            "common/*": ["node_modules/card-common/src/*"],
            "search/*": ["node_modules/card-search/src/*"],
            "search-data/*": ["node_modules/card-search/data/*"]
        },
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue"
    ]
}
